            sh:name "Schlagwort" ;
            sh:order 7 ;
            sh:path dcat:keyword ],
        [ skos:example "'Hybrid', 'gax:PrivateProvisioning'" ;
            sh:datatype xsd:string ;
            sh:description "Bereitstellungsart des Dienstes"@de,
